<?xml version="1.0" encoding="utf-8"?>
<Defs>
<!--===================================== CR shots =============================-->
<SoundDef>
<defName>Shot_TMC_CR</defName>  
 <context>MapOnly</context>   <eventNames />  
 <maxVoices>4</maxVoices>   <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
      <li Class="AudioGrain_Folder">
            <clipFolderPath>ColonyRifle</clipFolderPath>
          </li>
        </grains> <volumeRange>
          <min>50</min>        
          <max>60</max>
        </volumeRange>      
        <pitchRange>
          <min>0.97</min>        
          <max>1.03</max>
        </pitchRange>      
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>
